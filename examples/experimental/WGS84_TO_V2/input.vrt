<OGRVRTDataSource>
    <OGRVRTLayer name="input">
        <SrcDataSource>input.csv</SrcDataSource>
        <GeometryType>wkbPoint</GeometryType>
        <LayerSRS>+proj=longlat +datum=WGS84 +no_defs</LayerSRS>
        <GeometryField encoding="PointFromColumns" x="ETRS Longitude" y="ETRS89 Latitude"/>
    </OGRVRTLayer>
</OGRVRTDataSource>

